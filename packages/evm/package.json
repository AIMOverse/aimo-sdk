  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@aimo.network/client": "workspace:*",
    "@x402/core": "~2.1.0",
    "@x402/evm": "~2.1.0",
    "viem": "~2.43.3"
  },
  "devDependencies": {
    "rimraf": "~6.1.0",
    "typescript": "~5.9.0"
